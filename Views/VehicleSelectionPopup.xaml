<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:es="clr-namespace:Microsoft.Expression.Shapes;assembly=Microsoft.Expression.Drawing"
	mc:Ignorable="d"
	x:Class="car_pal.VehicleSelectionPopup"
	d:DesignWidth="480" d:DesignHeight="800">

	<Grid x:Name="LayoutRoot" Background="#BD000000" Margin="0,0,150,270">
		<StackPanel Margin="0,104,0,32" d:LayoutOverrides="VerticalAlignment">
			<Grid Height="100" Background="#FF646363">
				<Grid.RowDefinitions>
					<RowDefinition/>
					<RowDefinition/>
				</Grid.RowDefinitions>
				<TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Vehicle 1" VerticalAlignment="Top"/>
				<TextBlock HorizontalAlignment="Left" Margin="0,0,0,23" TextWrapping="Wrap" Text="Vehicle 2" d:LayoutOverrides="Height" Grid.Row="1"/>
			</Grid>
			<Button Content="Button" Click="Button_Click"/>
		</StackPanel>
	</Grid>
</UserControl>